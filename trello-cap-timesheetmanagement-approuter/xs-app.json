{
    "welcomeFile": "captrelloTimesheetManager/",
    "authenticationMethod": "route",
    "routes": [
        {
            "source": "^/timesheetService/(.*)$",
            "target": "/timesheet-management/$1",
            "authenticationType": "xsuaa",
            "destination": "srv_api",
            "csrfProtection": false
        },
        {
            "source": "^(.*)$",
            "target": "$1",
            "service": "html5-apps-repo-rt",
            "authenticationType": "xsuaa"
        }
    ]
}